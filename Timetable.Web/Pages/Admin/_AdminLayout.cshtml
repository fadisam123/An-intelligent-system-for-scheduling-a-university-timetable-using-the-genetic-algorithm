<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Admin - @ViewData["Title"]</title>
    <link rel="stylesheet" href="~/lib/bootstrap/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Timetable.RazorWeb.styles.css" asp-append-version="true" />

</head>
<body>
   <div>
    <div class="containeer">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    <div class="sidebar">
      <div class="icon">
        <a href="" class="sidebar-icon"><i class="bi bi-bell"></i></a>
        <a href="#" class="sidebar-icon">تسجيل الخروج</a>
        <a href="#" class="sidebar-icon"><i class="bi bi-gear"></i></a>
      </div>
      <h3>بوابة الإدارة</h3>
      <a asp-page="" class="sidebar-link">وضع إعلان</a>
      <a asp-page="" class="sidebar-link">فتح باب الاستبيان</a>
      <a asp-page="" class="sidebar-link">برنامج الدوام العملي</a>
      <a asp-page="./schedule" class="sidebar-link">برنامج الدوام النظري</a>
      <a asp-page="./teachers" class="sidebar-link" >اعضاء الهيئة التدريسية</a>
      <a asp-page="./practical-dept" class="sidebar-link">القسم العملي</a>
      <a asp-page="./theo-dept" class="sidebar-link">القسم النظري</a>
      <a asp-page="./class-room" class="sidebar-link">القاعات الدراسية</a>
      <a asp-page="./courses" class="sidebar-link">المواد الدراسية</a>
      <a asp-page="" class="sidebar-link">اوقات المحاضرات</a>
      <a asp-page="" class="sidebar-link">الطلاب</a>
   </div>
  </div>
    

    <script src="~/lib/jquery/jquery.min.js"></script>
    <script src="~/lib/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>